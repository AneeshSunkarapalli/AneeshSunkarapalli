import java.util.Scanner;

public class SameCharAsNameLength {

	public static void main(String[] args) {
		System.out.println("Enter name here: ");
		Scanner scan = new Scanner(System.in);
		String name = scan.next(); //allows for any name to be entered after its run
		
		for (int i = 0; i < name.length(); i++) {
			for (int j = 0; j < name.length(); j++) {

		char actualName = (name.charAt(i)); //creates a new char to be printed that repeats each letter to the length of the name
				System.out.print(actualName);

			}

		}
	}
}
